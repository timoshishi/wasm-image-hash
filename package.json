{
  "name": "wasm-img-hash",
  "version": "0.1.0",
  "description": "wasm perceptual image hashing module",
  "engines": "node >= 18.0.0",
  "main": "./dist/wasm-phash.umd.cjs",
  "module": "dist/index.es",
  "bin": "dist/index.cjs.js",
  "types": "typings/index.d.ts",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "files": [
    "dist",
    "typings",
    "rust/pkg"
  ],
  "scripts": {
    "build:rust": "cd rust && wasm-pack build --target nodejs --out-dir pkg",
    "build:ts": "tsc",
    "build": "npm run build:rust && npm run build:ts",
    "build:vite": "vite build",
    "prepublishOnly": "npm run build:vite",
    "test:wasm": "cd rust && wasm-pack test --node -- --nocapture"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "esm": "^3.2.25",
    "rollup-plugin-copy": "^3.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-top-level-await": "^1.3.0",
    "vite-plugin-wasm": "^3.2.2"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.3",
    "buffer": "^6.0.3",
    "node-fetch": "^3.3.1"
  }
}
